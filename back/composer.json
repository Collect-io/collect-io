{
    "name": "colllect/colllect",
    "license": "MIT",
    "type": "project",
    "description": "Colllect - Your new bookmark manager",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.8",
        "lcobucci/jwt": "^4.0",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-cached-adapter": "1.1.*",
        "league/flysystem-sftp": "^1.0",
        "nelmio/api-doc-bundle": "^3.5",
        "nyholm/psr7": "^1.2",
        "spatie/flysystem-dropbox": "^1.2",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.4",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/orm-pack": "^2.0",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/yaml": "4.4.*",
        "trikoder/oauth2-bundle": "3.2.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "nelmio/alice": "^3.5",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "0.12.95",
        "phpstan/phpstan-doctrine": "^0.12.43",
        "phpstan/phpstan-symfony": "^0.12.42",
        "rector/rector": "^0.11.49",
        "symfony/maker-bundle": "^1.14",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "apcu-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "cs": [
            "php-cs-fixer fix --diff"
        ],
        "phpstan": [
            "phpstan analyze"
        ],
        "rector": [
            "rector"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
